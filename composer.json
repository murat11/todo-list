{
  "type": "project",
  "require": {
    "php": "^7.2",
    "doctrine/dbal": "^2.10",
    "phpmailer/phpmailer": "^6.1",
    "twig/twig": "^3.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  }
}
